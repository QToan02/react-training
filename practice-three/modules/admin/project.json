{
  "name": "modules-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/admin/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["modules/admin/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/modules/admin"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/modules/admin"
      }
    }
  }
}
